#include <Arduino.h>
#include <WiFiClientSecure.h>
#include <ArduinoJson.h>
#include "config.h"
#include "secrets.h" 

//all setup functions
void setup(){
  
  setupSwitchLEDLights();
  setupNetwork();
  String RequestUrls[10];// Make Array Dynamic size
  GetUrlRequest(RequestUrls); // fxn should return an array of urls 
  for (int i=0; i< 10; i++)
      {
        String url =  RequestUrls[i];
        if (url.length() > 0)
        {
           Serial.println("Trying GitLabConnection or Url : " + url);
           results[i] = GitlabServerConnection(url); 
           Serial.println("Result from Url is : " + results[i]);
        }
      } 
    
  Serial.println("This is the program running");
}

//all loop functions
void loop(){ 
  for (int i=0; i< 10; i++)
  {
     pState = getPipelineStatus(results[i]);
     delay(5000);
     loopSwitchLEDLights(pState);
  }
}
